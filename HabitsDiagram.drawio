        <mxCell id="2v2APjPnS3a3vd251Anv-2" value="Менеджер должен иметь возможность создать комнату для участникав" style="whiteSpace=wrap;align=center;verticalAlign=middle;fontStyle=1;strokeWidth=3;fillColor=#FFFFCC" vertex="1" parent="1">
